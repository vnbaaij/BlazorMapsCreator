@page "/TilesetPage"
<h3>Create a Tileset</h3>

<Stack Horizontal="true" Tokens=@(new StackTokens() { ChildrenGap = new[] { 40.0 } }) Style="width:100%;">
    <StackItem>
        <DefaultButton @onclick="CreateTileset" Disabled="@createButtonDisabled">Create Tileset</DefaultButton>
    </StackItem>
    <StackItem>
        <DefaultButton @onclick="CreateTilesetStatus" Disabled="@statusButtonDisabled">Create Tileset Status</DefaultButton>
    </StackItem>
    <StackItem>
        <DefaultButton @onclick="GetMapTile" Disabled="@statusButtonDisabled">Get Map Tile</DefaultButton>
    </StackItem>
</Stack>

<h4>Values used:</h4>

<div>
    Selected geography: @(geography ?? "none")<br />
    Key: @subscriptionkey<br />
    <hr />
    Dataset Identifier: @datasetUdid<br />
    Tileset Identifier: @tilesetUdid<br />
</div>

<hr />
Map tile:

